<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <!-- new xml content goes here -->
            <block name="bluebird.order.export" template="Bluebird_OrderExport::export.phtml" ifconfig="sales/order_export/enabled">
                <arguments>
                    <argument name="details" xsi:type="object">Bluebird\OrderExport\ViewModal\OrderDetails</argument>
                </arguments>
            </block>
            <block name="bluebird.order.status" template="Bluebird_OrderExport::status.phtml" after="bluebird.order.export" />

        </referenceContainer>
    </body>
</page>
